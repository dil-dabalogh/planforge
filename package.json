{
  "name": "planforge",
  "version": "1.0.0",
  "description": "PlanForge - Project Planning and JIRA Integration Tool",
  "main": "index.html",
  "scripts": {
    "start": "npx http-server . -p 8080 -o",
    "dev": "npx http-server . -p 8080 -o -c-1",
    "serve": "python3 -m http.server 8080",
    "proxy": "node proxy-server.js",
    "start-with-proxy": "concurrently \"npm run proxy\" \"npm start\"",
    "dev-with-proxy": "concurrently \"npm run proxy\" \"npm run dev\""
  },
  "keywords": [
    "project-management",
    "planning",
    "jira",
    "timeline",
    "gantt"
  ],
  "author": "PlanForge Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "cors": "^2.8.5",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
